<?xml version="1.0" encoding="utf-8"?>
<!--
  For more information on how to configure your ASP.NET application, please visit
  http://go.microsoft.com/fwlink/?LinkId=169433
  -->
<configuration>
  <configSections>
    <sectionGroup name="couchbaseClients">
      <section name="couchbase" type="Couchbase.Configuration.Client.Providers.CouchbaseClientSection, Couchbase.NetClient" />
    </sectionGroup>
  </configSections>
  <system.web>
    <compilation targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
  </system.web>
  <appSettings>
    <add key="aldocV7url" value="http://easycar.aldoc.eu/MMT/mmt.asmx/"/>
    <add key="aldocV2url" value="http://easycar.aldoc.eu/ws/easycar/"/>
    <add key="cache.isenabled" value="false"/>
    <add key="cache.duration" value="0"/> <!-- 3 days -->
	<add key="timeout" value="5000"/>
  </appSettings>
  <system.webServer>
    <rewrite>
      <rules>
        <rule name="RedirectALXtoASHX" stopProcessing="true">
          <match url="^([^/]+)\.alx$" />
          <conditions logicalGrouping="MatchAll">
            <add input="{REQUEST_METHOD}" pattern="^GET$" />
          </conditions>
          <action type="Rewrite" url="V2Handler.ashx?rq={R:1}" appendQueryString="true" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
  <couchbaseClients>
    <couchbase useSsl="false">
      <servers>
        <add uri="http://127.0.0.1:8091/pools"></add>
      </servers>
      <buckets>
        <add name="default" useSsl="false" password="">
          <connectionPool name="custom" maxSize="10" minSize="5"></connectionPool>
        </add>
      </buckets>
    </couchbase>
  </couchbaseClients>
  
</configuration>